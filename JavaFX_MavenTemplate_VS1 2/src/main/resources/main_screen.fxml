<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Label?>

<VBox xmlns:fx="http://javafx.com/fxml/1" styleClass="vbox" fx:controller = "Main_menu_controller"
      fx:id = "root">
    <Label  fx:id = "titleLabel" text="Three Card poker" />
    <Button fx:id = "startGame" text = "Start" onAction = "#startGame"/>
    <Button fx:id = "quitGame" text = "Quit"  onAction =  "#quitGame"/>

</VBox>
